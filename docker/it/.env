# This file defines environment variables made available to docker-compose.yml
# by docker-compose up.
BASE_IMAGE=nlnetlabs/internet.nl:localbuild-it
COMPOSE_PROJECT_NAME=it

# Enable code coverage report generation (in /tmp/it-report)
ENABLE_COVERAGE=0


